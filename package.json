{
  "name": "folder hub",
  "version": "1.0.0",
  "description": "telegram bot to share your news feed and receive other people's in return",
  "main": "dist/app.js",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && node dist/app.js",
    "develop": "tsc-watch --skipLibCheck --onSuccess 'node dist/app.js'",
    "build": "tsc --skipLibCheck",
    "test": "yarn jest --forceExit --runInBand",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 ./src/",
    "lint-fix": "yarn eslint ./src/ --fix"
  },
  "dependencies": {
    "@grammyjs/i18n": "^0.5.0",
    "@grammyjs/menu": "^1.0.4",
    "@grammyjs/runner": "^1.0.3",
    "@types/node-telegram-bot-api": "^0.63.1",
    "dotenv": "^10.0.0",
    "envalid": "^7.2.2",
    "grammy": "^1.19.2",
    "grammy-middlewares": "^1.0.11",
    "mongoose": "^8.0.3",
    "node-telegram-bot-api": "^0.64.0"
  },
  "devDependencies": {
    "@typegoose/typegoose": "^12.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-no-relative-import-paths": "^1.0.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "module-alias": "^2.2.2",
    "node-telegram-bot-api": "^0.64.0",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.5.2"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "packageManager": "yarn@4.0.2"
}
